    "def xor(a, b): \n",
    "  \n",
    "    result = [] \n",
    "  \n",
    "    for i in range(1, len(b)): \n",
    "        if a[i] == b[i]: \n",
    "            result.append('0') \n",
    "        else: \n",
    "            result.append('1') \n",
    "  \n",
    "    return ''.join(result) "
